@page
@model ResetAuthenticatorModel
@{
    ViewData["Title"] = "تغییر توکن";
    ViewData["ActivePage"] = ManageNavPages.TwoFactorAuthentication;
}

<partial name="_StatusMessage" for="StatusMessage" />
<h3>@ViewData["Title"]</h3>
<div class="alert alert-warning" role="alert">
    <p>
        <span class="glyphicon glyphicon-warning-sign"></span>
        <strong>تغییر کلید دسترسی</strong>
    </p>
    <p>
        لطفا توسط اپلیکیشنی که از آن رمز دومرحله ای دریافت میکنید هم تغییرات را اعمال کنید درغیر این صورت رمز موفقت ایجاد شده کار نخواهد کرد
    </p>
</div>
<div>
    <form id="reset-authenticator-form" method="post">
        <button id="reset-authenticator-button" class="btn btn-danger" type="submit">ریست کردن توکن</button>
    </form>
</div>
